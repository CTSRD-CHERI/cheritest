if ($mips_gregs{"t0"} != 1) {
	return ("instruction before jal missed");
}
if ($mips_gregs{"t1"} != 2) {
	return ("instruction in branch-delay slot missed");
}
if ($mips_gregs{"t2"} != 0) {
	return ("jump didn't happen");
}
if ($mips_gregs{"t3"} != 4) {
	return ("instruction at jump target didn't run");
}
if ($mips_gregs{"t8"} != $mips_gregs{"ra"}) {
	my $ra_string = sprintf("0x%x", $mips_gregs{"ra"});
	my $t8_string = sprintf("0x%x", $mips_gregs{"t8"});
	return ("ra set to ".$ra_string." instead of expected ".$t8_string);
}
return ("OK");
